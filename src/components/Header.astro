---
import { Image } from 'astro:assets';
import Menu from './Menu.astro';
import MenuMobile from './MenuMobile.astro';
---

<header class="">
  <div class="mx-auto flex max-w-screen-lg items-center justify-between px-5">
    <a href="/">
      <Image
        src="/favicon.svg"
        alt="logo"
        width="36"
        height="36"
        class="h-[var(--header-height)]"
      />
    </a>

    <div>
      <Menu />
      <MenuMobile />
    </div>
  </div>
</header>
